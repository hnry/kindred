{
  "manifest_version": 2,
  "name": "kindred edit",
  "description": "Use your own editor instead of web editors.",
  "version": "0.0.1",
  "background": {
    "page": "background.html"
  },
  "page_action": {
    "default_icons": {
      "19": "icon48.png",
      "38": "icon48.png"
    },
    "default_title": "kindred edit",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "icons": {
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "permissions": [
    "*://*/*",
    "tabs",
    "storage",
    "nativeMessaging"
  ]
}
